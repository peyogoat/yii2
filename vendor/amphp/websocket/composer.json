{
    "name": "amphp/websocket",
    "description": "Async WebSocket client for PHP based on Amp.",
    "license": "MIT",
    "authors": [
        {
            "name": "Bob Weinand",
            "email": "bobwei9@hotmail.com"
        },
        {
            "name": "Aaron Piotrowski",
            "email": "aaron@trowski.com"
        },
        {
            "name": "Niklas Keller",
            "email": "me@kelunik.com"
        }
    ],
    "support": {
        "issues": "https://github.com/amphp/websocket/issues"
    },
    "keywords": [
        "async",
        "non-blocking",
        "websocket",
        "client",
        "http",
        "amp",
        "amphp"
    ],
    "require": {
        "php": ">=7",
        "amphp/amp": "^2",
        "amphp/byte-stream": "^1",
        "amphp/socket": "^0.10"
    },
    "require-dev": {
        "amphp/aerys": "^0.7.2",
        "amphp/phpunit-util": "^1",
        "friendsofphp/php-cs-fixer": "^2.9",
        "phpunit/phpunit": "^6"
    },
    "autoload": {
        "psr-4": {
            "Amp\\Websocket\\": "lib"
        },
        "files": [
            "lib/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Amp\\Websocket\\Test\\": "test"
        }
    },
    "config": {
        "platform": {
            "php": "7.0.0"
        }
    }
}
